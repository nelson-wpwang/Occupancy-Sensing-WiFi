    "def read_file(filename):\n",
    "    lines = []\n",
    "    with open(filename) as f:\n",
    "        while True:\n",
    "            line = f.readline()\n",
    "            components = line.split(\",\")\n",
    "            if line != '':\n",
    "                lines.append(components)\n",
    "            if not line:\n",
    "                break;\n",
    "    return lines"
   ]
  },
  {
